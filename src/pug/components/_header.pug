                    i.sprite.sprite-retina.icon-flag-french
                li
                  a(href='https://www.dezshira.com/international-desk/german.html')
                    i.sprite.sprite-retina.icon-flag-german
                li
                  a(href='https://www.dezshira.com/international-desk/italian.html')
                    i.sprite.sprite-retina.icon-flag-italian
                li
                  a(href='https://www.dezshira.com/international-desk/russian.html')
                    i.sprite.sprite-retina.icon-flag-russian
                li
                  a(href='https://www.dezshira.com/international-desk/spanish.html')
                    i.sprite.sprite-retina.icon-flag-spanish
                li
                  a(href='https://www.dezshira.com/cn')
                    i.sprite.sprite-retina.icon-flag-chinese
                li
                  a(href='https://www.dezshira.com/international-desk/australian.html')
                    i.sprite.sprite-retina.icon-flag-australian
          .panel.search
                .panelTab(data-toggle='tooltip', data-placement='bottom', title='', data-original-title='Search')
                  a.disable-links(href='javascript:;')
                    i.sprite.sprite-retina.icon-search
                .panelContent.searchtab
                  form.form-inline(action='https://www.dezshira.com/library')
                    .form-group
                      input#SearchInput.form-control(type='text', placeholder='Search', name='s')
                      i

